		"@vscode/test-electron": "^2.4.1",
		"esbuild": "^0.25.0",
		"eslint": "^9.21.0",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"axios": "^1.8.1",
		"cheerio": "^1.0.0",
		"error": "^10.4.0",
